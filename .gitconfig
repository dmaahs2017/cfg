[core]
    pager = delta
[delta]
    side-by-side = true
[user]
	name = dalton
	email = maahs2017@gmail.com
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[credential]
	helper = store
[diff]
	tool = vimdiff
[difftool]
	prompt = false
[alias]
	dt = difftool
	move-staged = "!bash -c \"git stash-staged;git commit -m \"temp\"; git stash; git reset --hard HEAD^; git stash pop\""
[merge]
	tool = nvimdiff
[mergetool]
	prompt = false
[github]
	user = dmaahs2017
